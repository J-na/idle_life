[editor_plugins]

enabled=PackedStringArray("res://addons/copilot-advanced/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "red",
"res://data_structures/": "yellow",
"res://scenes/": "green",
"res://scripts/": "purple"
}
